<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<doc>
  <assembly>
    <name>System.Messaging</name>
  </assembly>
  <members>
    <member name="Overload:System.Messaging.Design.QueuePathEditor.EditValue">
      <summary>Edits the value of the specified message queue, using the specified service provider.</summary>
    </member>
    <member name="Overload:System.Messaging.Design.QueuePathEditor.GetEditStyle">
      <summary>Returns the editing style used by the <see cref="Overload:System.Messaging.Design.QueuePathEditor.EditValue" /> method.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageEnumerator.Dispose">
      <summary>Releases the resources used by the <see cref="T:System.Messaging.MessageEnumerator" />.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageEnumerator.MoveNext">
      <summary>Advances the enumerator to the next message in the queue.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageEnumerator.RemoveCurrent">
      <summary>Removes the current message from the queue and returns the message to the calling application. Removing the message deletes it from the queue.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueueEnumerator.Dispose">
      <summary>Releases the resources used by the <see cref="T:System.Messaging.MessageQueueEnumerator" />.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueuePermissionEntryCollection.AddRange">
      <summary>Appends a set of specified permission entries to this collection.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueueTransaction.Dispose">
      <summary>Releases the resources used by the <see cref="T:System.Messaging.MessageQueueTransaction" />.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.BeginPeek">
      <summary>Initiates an asynchronous peek operation by telling Message Queuing to begin peeking a message and notify the event handler when finished.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.BeginReceive">
      <summary>Initiates an asynchronous receive operation by telling Message Queuing to begin receiving a message and notify the event handler when finished.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.Create">
      <summary>Creates a new queue at the specified path on a Message Queuing server.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.Dispose">
      <summary>Disposes of the resources (other than memory) used by the <see cref="T:System.Messaging.MessageQueue" />.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.GetMessageQueueEnumerator">
      <summary>Creates an enumerator object for a dynamic listing of the public queues on the network.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.GetPublicQueues">
      <summary>Retrieves all the public queues on the network.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.Peek">
      <summary>Returns a copy of the first message in the queue without removing the message from the queue.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.PeekByCorrelationId">
      <summary>Peeks a message that matches a given correlation identifier.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.PeekById">
      <summary>Returns a copy of the message that has the specified message identifier without removing the message from the queue.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.PeekByLookupId">
      <summary>Introduced in MSMQ 3.0. Peeks at a specific message from the queue. The message can be specified by a lookup identifier or by its position at the front or end of the queue.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.Receive">
      <summary>Receives the first message in the queue, removing it from the queue.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.ReceiveByCorrelationId">
      <summary>Receives a message that matches a given correlation identifier.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.ReceiveById">
      <summary>Receives the message that matches the given identifier, removing it from the queue.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.ReceiveByLookupId">
      <summary>Introduced in MSMQ 3.0. Receives a specific message from the queue. The message can be specified by a lookup identifier or by its position at the front or end of the queue.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.Send">
      <summary>Sends an object to a queue.</summary>
    </member>
    <member name="Overload:System.Messaging.MessageQueue.SetPermissions">
      <summary>Adds permissions to the current set. This controls who has access rights to queue properties and messages in the queue.</summary>
    </member>
  </members>
</doc>